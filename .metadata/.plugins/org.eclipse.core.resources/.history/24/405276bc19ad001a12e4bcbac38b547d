package fundamentos;

public class Temperatura {
	
	public static void main(String[] args) {
		final int TEMP_CONVERSAO = 32;
		final double TAXA_CONVERSAO = 0.5556;
		float TempFahrenheit = 145.56f;
//		float TempCelsius = 0.0f;
		double TempConvertida = (TempFahrenheit - TEMP_CONVERSAO) * (TAXA_CONVERSAO);
//		TempCelsius = TempConvertida;
		System.out.println("A temperatura " + TempFahrenheit + " °F para Celsius é: " + TempConvertida + " °C" );
		
	}
}
